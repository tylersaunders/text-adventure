{
    "compilerOptions": {
        "baseUrl": ".",
        "forceConsistentCasingInFileNames": true,
        "incremental": true,
        "lib": [
            "esnext",
            "dom"
        ],
        "moduleResolution": "node",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "removeComments": true,
        "sourceMap": true,
        "strict": true,
        "target": "esnext"
    }
}
